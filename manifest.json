{
    "name": "Slightly Improved Tab Groups",
    "version": "1.0.0",
    "description": "Make new tab group tabs inherit the first tabs container",
    "manifest_version": 3,
    "icons": {
        "128": "icon128.png"
    },
    "permissions": [
        "tabs",
        "cookies"
    ],
    "action": {
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
